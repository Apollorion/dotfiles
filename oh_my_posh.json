{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#bd93f9",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "agnoster_full"
          },
          "style": "diamond",
          "type": "path"
        },
        {
          "background": "#ffb86c",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " \ue725 ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "background": "#8be9fd",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#ff79c6",
          "foreground": "#f8f8f2",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": " \u2665 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "kubectl",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "powerline_symbol": "",
          "foreground": "#000000",
          "background": "#ebcc34",
          "template": " ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "properties": {
            "context_aliases": {
              "arn:aws:eks:us-east-2:833241423620:cluster/joey": "joeys effectual cluster"
            }
          }
        },
        {
          "background": "#f1fa8c",
          "foreground": "#282a36",
          "invert_powerline": true,
          "style": "diamond",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "trailing_diamond": "\ue0b4",
          "type": "aws"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFFFFF",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "$ ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false,
  "version": 2
}
