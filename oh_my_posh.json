{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_full"
          },
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#bd93f9",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "template": " \ue725 ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#ffb86c",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#8be9fd",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "trailing_diamond": "\ue0b0",
          "template": " â™¥ {{ .CurrentDate | date .Format }} ",
          "foreground": "#f8f8f2",
          "background": "#ff79c6",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "context_aliases": {
              "arn:aws:eks:us-east-2:833241423620:cluster/joey": "joeys effectual cluster"
            }
          },
          "leading_diamond": "\ue0b2",
          "template": " \ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#ebcc34",
          "type": "kubectl",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "#282a36",
          "background": "#f1fa8c",
          "type": "aws",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "$ ",
          "foreground": "#FFFFFF",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
